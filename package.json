{
  "name": "soldatbr-bot",
  "version": "0.0.1",
  "description": "A Soldat Brazilian BOT competitive manager",
  "main": "dist/init.js",
  "scripts": {
    "test": "mocha --exit",
    "build:test": "npm run build && npm run test",
    "build": "babel app -d dist",
    "start": "node app/init.js",
    "debug": "nodemon --watch app --exec node --inspect app/init.js",
    "build:dist": "babel app -d dist --source-maps=false",
    "build:debug": "npm run build && node --inspect dist/init.js",
    "serve": "nodemon --watch app --exec npm run build:debug"
  },
  "keywords": [
    "soldat",
    "soldatbr",
    "bot"
  ],
  "author": "Luis Fedrizze <luisfedrizze@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^12.2.0",
    "mongodb": "^3.5.9",
    "slugify": "^1.4.4",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4"
  }
}
